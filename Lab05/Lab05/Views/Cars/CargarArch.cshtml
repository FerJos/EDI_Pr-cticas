@model List<DataCarsModel>
@{
    ViewData["Title"] = "Carga por CSV";
}
<h2>@ViewData["Title"]</h2>

@using(Html.BeginForm("CargarArch","Cars",FormMethod.Post,new {enctype = "multipart/form-data"}))
{
    <label for="file"> Subir Archivo </label>
    <input type ="file" name = "file" id="file"/>
    <input type ="submit" value="Enviar"/>
}

@if(Model !=null)
{
    <table class = "table table-hover">
    <thead>
        <tr>
            <td>Placa</td>
            <td>Color</td>
             <td>Propietario</td>
              <td>CoordenadasLat</td>
               <td>CoordenadasLong</td>
        </tr>
    </thead>
@foreach(var persona in Model)
    {
        <tr>
            <td>@persona.Placa</td>
            <td>@persona.Color</td>
              <td>@persona.Propietario</td>
                <td>@persona.CoordenadasLat</td>
                  <td>@persona.CoordenadasLong</td>
        </tr>
    }
</table>
}
