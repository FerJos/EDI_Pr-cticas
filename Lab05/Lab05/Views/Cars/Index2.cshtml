@model IEnumerable<Lab05.Models.DataCarsModel>
@{
    ViewData["Title"] = "Personas";
}
<h2>@ViewData["Title"]</h2>
<p>Lista de personas cargadas en memoria: </p>

<table class = "table table-hover">
    <thead>
        <tr>
            <td>ID</td>
            <td>Color</td>    
            <td>Propietario</td>
            <td>Latitud</td>
            <td>Longitud</td>
        </tr>
    </thead>
@foreach(var carros in Model)
    {
        <tr>
            <td>@carros.Placa</td>
            <td>@carros.Color</td>
            <td>@carros.Propietario</td>
            <td>@carros.CoordenadasLat</td>
            <td>@carros.CoordenadasLong</td>
        </tr>
    }
</table>